mixin imgslide (items)
    div.sl 
        for item in items 
            div.divsl
                img(src=item.img)
                h4 #{item.name}
                p #{item.job}
                ul.social
                    li 
                        a(href=item.facebook).link.facebook 
                    li 
                        a(href=item.twitter).link.twitter
                    li 
                        a(href=item.googlePlus).link.googlePlus
                    li 
                        a(href=item.linkedIn).link.linkedIn
