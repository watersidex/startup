mixin bottomcard (items)
    .bottomcards
        for item in items 
            .blog
                img(src=item.img)
                .blogtx
                    .blogtitle
                        .blogdate
                            strong #{item.num}
                            span #{item.month}
                        .txtitle 
                            h4 #{item.name}
                            p #{item.p}
                    p #{item.tx}
                    p.txmore #{item.add}
                    a(href='#').readmore #{item.more}
